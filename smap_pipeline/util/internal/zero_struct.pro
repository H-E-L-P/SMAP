id=sl_svn20_43><td class="source">PRO zero_struct, struct<br></td></tr
><tr
id=sl_svn20_44><td class="source"><br></td></tr
><tr
id=sl_svn20_45><td class="source">  IF N_params() EQ 0 THEN BEGIN <br></td></tr
><tr
id=sl_svn20_46><td class="source">     print,&#39;-Syntax: zero_struct, struct&#39;<br></td></tr
><tr
id=sl_svn20_47><td class="source">     print,&#39;&#39;<br></td></tr
><tr
id=sl_svn20_48><td class="source">     print,&#39;Use doc_library,&quot;zero_struct&quot;  for more help.&#39;  <br></td></tr
><tr
id=sl_svn20_49><td class="source">     return<br></td></tr
><tr
id=sl_svn20_50><td class="source">  ENDIF <br></td></tr
><tr
id=sl_svn20_51><td class="source"><br></td></tr
><tr
id=sl_svn20_52><td class="source">  IF size(struct,/tname) NE &#39;STRUCT&#39; THEN BEGIN<br></td></tr
><tr
id=sl_svn20_53><td class="source">      message,&#39;Input value must be a structure.  Input variable unchanged&#39;,/inf<br></td></tr
><tr
id=sl_svn20_54><td class="source">      return<br></td></tr
><tr
id=sl_svn20_55><td class="source">  ENDIF <br></td></tr
><tr
id=sl_svn20_56><td class="source">  <br></td></tr
><tr
id=sl_svn20_57><td class="source">  ;; Make a structure with a random variable name and use struct_assign <br></td></tr
><tr
id=sl_svn20_58><td class="source">  ;; to &quot;copy&quot; between structures.  By default, fields that do not match<br></td></tr
><tr
id=sl_svn20_59><td class="source">  ;; are zeroed<br></td></tr
><tr
id=sl_svn20_60><td class="source"><br></td></tr
><tr
id=sl_svn20_61><td class="source">  tagname = &#39;randomFront&#39;<br></td></tr
><tr
id=sl_svn20_62><td class="source">  numstr =  $<br></td></tr
><tr
id=sl_svn20_63><td class="source">    strtrim(string(ulong64(1000*systime(1))), 2) + &#39;moreRandom&#39; + $<br></td></tr
><tr
id=sl_svn20_64><td class="source">    strtrim(string(long(1000000*randomu(seed))), 2)<br></td></tr
><tr
id=sl_svn20_65><td class="source"><br></td></tr
><tr
